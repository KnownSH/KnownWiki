{
  "extends": "astro/tsconfigs/strict",
  "include": [
    "src/mdx.d.ts"
  ],
  "compilerOptions": {
    "allowImportingTsExtensions": true,
    "jsx": "react-jsx",
    "jsxImportSource": "react",
    "baseUrl": ".",
    "paths": {
      "@/*": [
        "src/*"
      ],
      "@components/*": ["src/components/*"],
      "@assets/*": ["src/assets/*"],
    }
  }
}